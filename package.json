    "command-line-args": "^5.0.2",
    "create-angular-template": "^1.1.0",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^7.1.1",
